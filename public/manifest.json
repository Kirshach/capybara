{
  "manifest_version": 2,
  "name": "Capybara",
  "description": "Chrome extension of Capybara app",
  "version": "0.1.0",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage", "tabs", "activeTab", "bookmarks", "cookies"],
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "64": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon2.png",
      "32": "icon2.png"
    },
    "default_title": "Capybara popup",
    "default_popup": "popup.html"
  }
}
